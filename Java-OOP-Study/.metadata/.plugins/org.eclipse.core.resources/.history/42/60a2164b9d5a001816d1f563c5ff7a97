package com.study.innerClass;

public class AnonymousInnerClassDemo {
public static void main(String[] args) {
	Inner1 i1 = new Inner1Imp1();
	
	/**
	 * 1.写了一个实现类，实现Inner1接口
	 * 2.给该实现类创建了一个对象
	 * 3.该实现类的结构
	 */
	Inner1 i2 = new Inner1() {
		
	};
}

}

interface Inner1{
	
}

class Inner1Imp1 implements Inner1{
	
}